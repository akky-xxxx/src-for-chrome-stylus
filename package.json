{
  "name": "src-for-chrome-stylus",
  "version": "1.0.0",
  "scripts": {
    "build": "run-p build:*",
    "build:github": "parcel watch ./src/github/index.scss -d ./dist/github --no-source-maps",
    "fix": "run-p fix:*",
    "fix:pack": "fixpack",
    "fix:scss": "stylelint --fix './src/**/*.scss'",
    "lint": "run-p lint:*",
    "lint:scss": "stylelint './src/**/*.scss'"
  },
  "devDependencies": {
    "fixpack": "^2.3.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-sorting": "^5.0.1",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.23.3",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.12.0"
  },
  "author": "Shogo Akimoto <114.s.aki@gmail.com>",
  "private": true,
  "repository": "https://github.com/akky-xxxx/src-for-chrome-stylus.git",
  "license": "MIT"
}
